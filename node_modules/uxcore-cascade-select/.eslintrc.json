{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "react/jsx-no-bind": "off",
    "no-plusplus": "off"
  },
  "globals": {
    "it": true,
    "describe": true,
    "beforeEach": true,
    "afterEach": true
  }
}